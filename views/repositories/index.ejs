<div class="col-xs-10 col-xs-offset-1">
  <section id="repositories">
      <tbody>


        <% users.forEach(function(user){ %>

            <td class="text-center"> <span class="badge"><%= user.username %></span></td>
            <button type="button" onclick="userInfo('<%= user.username %>')" name="button">User Info</button>


          <% }) %>
          <button type="button" src="/maps" name="button"></button>
      </tbody>
  </section>
</div>
